{
  "Consul": {
    "Host": "localhost", // Consul sunucusunun adresi (bu örnekte yerel makine)
    "Port": 8500, // Consul sunucusunun port numarası (standart port 8500)
    "Discovery": {
      "ServiceName": "first-service", // Servisin Consul içinde kayıtlı olan adı
      "Hostname": "localhost", // Servisin hostname'i (bu, diğer servislerin bu servisi bulması için kullanılır)
      "Port": 5176 // Servisin çalıştığı port numarası

    }
  },


  "MicroserviceBaseUrls": {
    "MicroserviceSecond": "https://second-service"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}